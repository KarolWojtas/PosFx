<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<HBox fx:id="reportItemRoot" alignment="CENTER" maxWidth="1.7976931348623157E308" minHeight="0.0" minWidth="0.0" stylesheets="@../../styles/orderList.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.karol.posfx.controllers.ReportItemController">
   <children>
      <VBox alignment="BOTTOM_LEFT" HBox.hgrow="ALWAYS">
         <children>
            <Label fx:id="reportItemNameLbl" maxWidth="1.7976931348623157E308" text="Rodzaj" textFill="WHITE">
               <font>
                  <Font size="18.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="5.0" right="-50.0" />
               </VBox.margin>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </Label>
            <Label fx:id="reportItemDescriptionLbl" text="jednostka" textFill="WHITE">
               <padding>
                  <Insets bottom="5.0" left="10.0" right="5.0" />
               </padding>
            </Label>
         </children>
      </VBox>
      <StackPane maxHeight="1.7976931348623157E308" HBox.hgrow="NEVER">
         <children>
            <Circle fx:id="reportItemValueCircle" fill="#7082d32e" radius="30.0" stroke="TRANSPARENT" strokeType="INSIDE" strokeWidth="3.0" style="-fx-border-color: blue; -fx-border-width: 2;" />
            <Text fx:id="reportItemValueText" fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="Text">
               <font>
                  <Font name="System Bold" size="24.0" />
               </font>
            </Text>
         </children>
      </StackPane>
   </children>
</HBox>
