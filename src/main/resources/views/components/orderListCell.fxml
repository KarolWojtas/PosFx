<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Circle?>


<VBox fx:id="orderCellRoot" alignment="CENTER" stylesheets="@../../styles/orderList.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox fx:id="orderCellContentRoot">
         <children>
            <HBox fx:id="orderCellHeaderRoot">
               <children>
                  <VBox fx:id="orderCellLeftInfoRoot" HBox.hgrow="ALWAYS">
                     <children>
                        <Label fx:id="orderCellCreatedLbl" maxWidth="1.7976931348623157E308" text="Label" />
                        <Label fx:id="orderCellOrdererLbl" text="Label" />
                     </children>
                  </VBox>
                  <StackPane fx:id="orderCellPriceRoot" HBox.hgrow="ALWAYS">
                     <children>
                        <Circle fx:id="orderCellPriceCircle" fill="DODGERBLUE" radius="30.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0" />
                        <Label fx:id="orderCellPriceLbl" text="Label" />
                     </children>
                  </StackPane>
               </children>
            </HBox>
            <ListView fx:id="orderCellProductsListView" />
         </children>
      </VBox>
   </children>
</VBox>
